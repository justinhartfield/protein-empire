{
  "name": "protein-empire",
  "version": "1.0.0",
  "description": "The Protein Empire - A network of macro-verified protein recipe websites",
  "private": true,
  "type": "module",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "build": "node scripts/build-all.js",
    "build:site": "node scripts/build-site.js",
    "generate:pdfs": "node scripts/generate-pdfs.js",
    "generate:social": "node packages/social-media/src/generate-all.js",
    "generate:emails": "node packages/email-marketing/src/generate-campaigns.js",
    "generate:analytics": "node packages/analytics/src/setup-analytics.js",
    "generate:calendar": "node packages/content-calendar/src/generate-calendar.js",
    "dashboard": "node packages/marketing-dashboard/src/server.js",
    "dev": "node scripts/dev-server.js",
    "lint": "eslint .",
    "format": "prettier --write ."
  },
  "keywords": [
    "protein",
    "recipes",
    "fitness",
    "baking",
    "macros"
  ],
  "author": "Protein Empire",
  "license": "UNLICENSED",
  "devDependencies": {
    "eslint": "^8.56.0",
    "prettier": "^3.2.0"
  },
  "dependencies": {
    "@netlify/blobs": "^8.1.0",
    "dotenv": "^17.2.3",
    "ejs": "^3.1.10",
    "gray-matter": "^4.0.3",
    "marked": "^11.1.0",
    "puppeteer": "^21.7.0",
    "sharp": "^0.33.2"
  },
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  }
}
