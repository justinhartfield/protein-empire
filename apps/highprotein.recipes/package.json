{
  "name": "@protein-empire/highprotein-recipes",
  "version": "2.0.0",
  "private": true,
  "description": "Protein Empire hub site: highprotein.recipes - Dotoro integration enabled",
  "scripts": {
    "build": "cd ../.. && node scripts/build-site.js highprotein.recipes",
    "build:dotoro": "cd ../.. && python3 scripts/build_highprotein_recipes.py",
    "build:feed": "cd ../.. && python3 packages/page-generator/create_master_feed.py",
    "build:landing": "cd ../.. && python3 packages/page-generator/generate_landing_pages.py && python3 packages/page-generator/generate_macro_pages.py",
    "build:sitemap": "cd ../.. && python3 packages/page-generator/generate_sitemap.py"
  },
  "dependencies": {
    "@sendgrid/client": "^8.1.1",
    "@sendgrid/mail": "^8.1.1"
  }
}
